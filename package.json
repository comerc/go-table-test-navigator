{
  "name": "go-table-test-navigator",
  "publisher": "comerc",
  "displayName": "Go Table Test Navigator",
  "icon": "images/icon.png",
  "description": "Navigate to specific table test cases in Go",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/comerc/go-table-test-navigator"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "go",
    "golang",
    "table test",
    "navigation",
    "testing"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "goTableTestNavigator.goToTableTestCase",
        "title": "Go to Table Test Case",
        "icon": "$(go-to-file)"
      }
    ],
    "menus": {
      "testing/item/context": [
        {
          "command": "goTableTestNavigator.goToTableTestCase",
          "when": "testId =~ /.*#.*\\/.*/ && resourceExtname == .go"
        }
      ],
      "commandPalette": [
        {
          "command": "goTableTestNavigator.goToTableTestCase",
          "when": "false"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "22.x",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.4",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^8.47.0",
    "glob": "^10.3.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4"
  }
}